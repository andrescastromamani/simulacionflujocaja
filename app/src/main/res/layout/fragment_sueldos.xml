<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    tools:context=".IueFragment">

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:fillViewport="true"
        android:scrollbarStyle="outsideInset"
        android:scrollbars="horizontal|vertical">

        <HorizontalScrollView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:scrollbars="horizontal|vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="395dp"
                        android:layout_height="match_parent"
                        android:layout_margin="10dp"
                        android:text="Sueldos y Salarios"
                        android:textAlignment="center"
                        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                        android:textSize="20dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="261dp">

                    <TextView
                        android:id="@+id/textView1"
                        android:layout_width="wrap_content"
                        android:layout_height="50dp"
                        android:layout_margin="10dp"
                        android:text="Aportes Patronales"
                        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="51dp"
                        android:layout_margin="10dp"
                        android:text="Caja"
                        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

                    <TextView
                        android:layout_width="53dp"
                        android:layout_height="50dp"
                        android:layout_margin="10dp"
                        android:text="Riesgo"
                        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                        android:translationX="20dp" />

                    <EditText
                        android:layout_width="46dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="15dp"
                        android:hint=""
                        android:singleLine="false"
                        android:translationX="-215dp"
                        android:translationY="60dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="47dp"
                        android:layout_margin="10dp"
                        android:text="Sol."
                        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                        android:translationX="-45dp" />

                    <EditText
                        android:id="@+id/editText3"
                        android:layout_width="46dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="15dp"

                        android:hint=""
                        android:singleLine="false"
                        android:translationX="-252dp"
                        android:translationY="60dp" />

                    <EditText
                        android:layout_width="46dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="15dp"
                        android:hint=""
                        android:translationX="-249dp"
                        android:translationY="60dp" />

                    <TextView
                        android:id="@+id/textView18"
                        android:layout_width="wrap_content"
                        android:layout_height="47dp"
                        android:layout_margin="10dp"
                        android:text="Prov."
                        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                        android:translationX="-169dp" />

                    <EditText
                        android:id="@+id/editText"
                        android:layout_width="46dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="15dp"
                        android:baselineAligned="false"
                        android:hint=""
                        android:singleLine="false"
                        android:translationX="-302dp"
                        android:translationY="60dp" />

                    <EditText
                        android:layout_width="46dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="15dp"
                        android:hint=""
                        android:singleLine="true"
                        android:translationX="-305dp"
                        android:translationY="60dp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <EditText
                        android:id="@+id/editText13"
                        android:layout_width="46dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="15dp"
                        android:hint=""
                        android:singleLine="false"
                        android:translationX="-205dp"
                        android:translationY="60dp" />

                    <EditText
                        android:id="@+id/editText11"
                        android:layout_width="56dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="10dp"
                        android:baselineAligned="false"
                        android:hint=""
                        android:singleLine="false"
                        android:translationX="-310dp"
                        android:translationY="75dp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="151dp">

                    <EditText
                        android:layout_width="100dp"
                        android:layout_height="match_parent"
                        android:layout_margin="10dp"
                        android:hint="" />

                    <EditText
                        android:layout_width="100dp"
                        android:layout_height="match_parent"
                        android:layout_margin="10dp"
                        android:hint="" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="138dp">

                    <EditText
                        android:layout_width="100dp"
                        android:layout_height="110dp"
                        android:layout_margin="10dp"
                        android:hint="" />

                    <EditText
                        android:layout_width="85dp"
                        android:layout_height="68dp"
                        android:layout_margin="10dp"
                        android:hint="" />

                    <EditText
                        android:layout_width="79dp"
                        android:layout_height="68dp"
                        android:layout_margin="10dp"
                        android:hint="" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                </LinearLayout>

                <LinearLayout
                    android:layout_width="719dp"
                    android:layout_height="508dp" />

            </LinearLayout>
        </HorizontalScrollView>
    </ScrollView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
</LinearLayout>